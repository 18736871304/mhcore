    <style>
        .title {
            width: 100%;
            height: auto;
            background: #fff;
        }

        .banner_wrapper {
            width: 100%;
            height: auto;
        }

        .banner_desc p {
            width: 100%;
            height: 2.25rem;
            line-height: 2.25rem;
            overflow: hidden;
            padding: 0 0.75rem;
            background: #fff;
            color: #686868;
            box-sizing: border-box;
        }

        .banner_desc p {
            border-bottom: 1px solid #e8e8e8;
        }

        .banner_desc p span {
            font-size: 0.7rem;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }

        .box-beijing {
            width: 100%;
            height: 0.5rem;
            background-color: #f0efef;
            /* border-top: 1px solid #d8d8d8; */
        }

        .wei-clause {
            padding-left: 0.75rem;
            padding-right: 0.75rem;
        }

        .banner_desc .wei-clause a p {
            padding-left: 0;
            padding-right: 0;
        }

        .wei-clause a p {
            display: flex;
            justify-content: space-between;
            border-bottom: 1px dashed #d8d8d8;
        }

        .wei-clause a .last-child {
            border-bottom: 0px dashed #d8d8d8;
        }

        .banner_desc p .zhu-size {
            font-size: 0.8rem;
            color: #222;
        }


        .no-bottom {
            border-bottom: 0px dashed #d8d8d8;
        }

        .ze-button {
            position: fixed;
            left: 0;
            right: 0;
            bottom: 0;
            background: #dc220d;
            width: 100%;
            height: 2.8rem;
            line-height: 2.8rem;
            text-align: center;
            color: #ffffff;
            z-index: 510;
            font-size: 0.8999rem;
        }

        .title {
            position: fixed;
            left: 0;
            right: 0;
            bottom: 0;
            width: 100%;
            height: 22.75rem;
            background-color: #fff;
            display: none;
            z-index: 1002;
            overflow-y: scroll;
            box-sizing: border-box;
        }
    </style>
</head>

