  font-size: 0.3rem;
  text-align: center;
}


.secrecy {
  width: 100%;
  height: auto;
}

.secrecy p {
  width: 100%;
  height: auto;
  font-size: 0.2rem;
  color: #979797;
  text-align: justify;
}

.secrecy p::before {
  content: "*";
  display: inline-block;
  height: 0.1rem;
  width: 0.1rem;
  color: #e12914;
}

.errorMsg,
.phoneError,
.userError {
  height: 0.32rem;
  font-size: 0.24rem;
  color: #e12914;
  display: block;
  visibility: hidden;
  margin-left: 0.2rem;
}





