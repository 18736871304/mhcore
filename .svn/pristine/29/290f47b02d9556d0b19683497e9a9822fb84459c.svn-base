@charset "utf-8";

html {
	font-size: calc(100vw/ 18.75);
	font-family: "微软雅黑";
	max-width: 750px;
	margin: auto;
}

@media screen and (min-width:750px) {
	html {
		font-size: 21px;
	}
}

input {
	-webkit-appearance: none;
	-webkit-border-radius: 0;
	-webkit-user-select: text;
	caret-color: #F76F6F;
	border:0;
	background:none;
	outline:none
}
a {
	text-decoration: none;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
	-webkit-user-select: none;
	-moz-user-select: none;
}

* {
	box-sizing: border-box;
	margin: 0;
	padding: 0;
}
.input-right{
	text-align:right
}
.flex-all{
	flex:auto;
}
.bg-arrow{
	background: url(../../images/car/car-arrow-forward.png) right center no-repeat;
	    background-size:0.3rem;
}
.banner img{
	display:block;
	width:100%;
}
.car-wrap .info-wrap{
	margin:0.5rem 0.25rem 0;
    box-shadow: 0 0 10px 2px rgba(0,0,0,.1);
    padding:0 0.75rem;
}
.car-wrap .info-wrap .item {
	position:relative;
}
.car-wrap .info-wrap .item .errMsg{
	position: absolute;
    right: 0;
    bottom: 0.1rem;
    line-height: 1;
    font-size: 0.5rem;
    text-align: right;
    color: #F76F6F;
    display:none
}
.car-wrap .info-wrap .item input{
	padding-right:1rem
}
.car-wrap .info-wrap .item:last-child{
		border:0!important;
}
.car-wrap .info-wrap .item.flex{
	display:flex;
	align-items:center;
	height:2.2rem;
	border-bottom: 1px dashed #d8d8d8;
} 
.car-wrap .info-wrap .item .icon-box{
	width:0.8rem;
	height:0.75rem;
	margin-right:0.4rem;
}
.car-wrap .info-wrap .item .icon-box span{
	background-size: 0.8rem auto;
    display:block;
    margin: 50% auto 0;
    transform:translateY(-50%)
}
.car-wrap .info-wrap .item .icon1{
	background: url(../../images/car/car-icon-sprite.png) -0 -0.8rem no-repeat;
	height: 0.75rem;
	width: 0.8rem;
}
.car-wrap .info-wrap .item .icon2{
	background: url(../../images/car/car-icon-sprite.png) -0 -2.225rem no-repeat;
	height: 0.6rem;
	width: 0.725rem;
}
.car-wrap .info-wrap .item .icon3{
	background: url(../../images/car/car-icon-sprite1.png) -0 -1.67rem no-repeat;
	height: 0.7rem;
	width: 0.7rem;
}
.car-wrap .info-wrap .item .icon4{
	background: url(../../images/car/car-icon-sprite.png) -0 -0 no-repeat;
	height: 0.75rem;
	width: 0.5rem;
}
.car-wrap .info-wrap .item .name{
	font-size:0.7rem;
	color:#333333
}
.carNumber input.pro{
	    width: 1.7rem;
}
.carNumber input.num{
	    width: 6rem;
    text-align: right;
}
.car-wrap .sub-btn{
	line-height:2.45rem;
	text-align:center;
	color:#fff;
	font-size:0.8rem;
	background:#b71c0b;
	position:fixed;
	bottom:0;
	left:0;
	width:100%
}
.success-tips-dialog{
	position:fixed;
	left:0;
	right:0;
	bottom:0;
	top:0;
		display:none
	
}
.success-tips-dialog .wrap{
	width:12.25rem;
	height:8.1rem;
	background:#fff;
	position:absolute;
	left:0;
	right:0;
	bottom:0;
	top:0;
	margin:auto;
	border-radius:0.4rem;
}
.success-tips-dialog .wrap .close-dialog{
	    position: absolute;
    right: 0.3rem;
    top: -0.1rem;
    color: #898989;
    font-size: 1.6rem;
}
.success-tips-dialog .wrap .txt{
	    font-size: 0.8rem;
    line-height: 2;
    margin-top: 2rem;
    border-top: 1px solid #e1e1e1;
    padding: 1.3rem 0;
    text-align: center;
}
#appntArea{
	width:auto;
}
input{
	font-size: 0.7rem;
}
