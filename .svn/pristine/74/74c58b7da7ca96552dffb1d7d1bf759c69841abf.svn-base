  }
} */
.xieyi {
  margin-top: 0.1rem;
  font-size: 0.13rem;
  color: #999;
  text-align: justify;
}

.bigBtn {
  cursor: pointer;
  position: relative;
  padding-bottom: 0.5rem;
}

.bigBtn img {
  width:6.4rem;
 height: 1rem;
  display: block;
  margin: 0 auto;
}

.moudle3 .moudle-content {
  padding-bottom: 0.3rem;
}

.lunbo {
  width:90%;
  padding: 0.4rem;
  margin: 0 auto;
  border: 2px solid #eaeaea;
  position: relative;
  border-radius: 0.3rem;
}



.swiper-slide {
  text-align: center;
  font-size: 18px;
  background: #fff;

  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.swiper-slide img {
  display: block;
  width: 100%;
  height: 100%;
  object-fit: cover;
}



.swiper-button-next {
  right: 5px;
  left: auto;
}

.swiper-button-prev {
  left: 5px;
  right: auto;
}

.swiper-button-next:after {
  width: 0.3rem;
  height: 0.3rem;
  content: '';
  background: url(../images/bg-left.png) no-repeat center center;
  background-size: 100% 100%;
}

.swiper-button-prev:after {
  width: 0.3rem;
  height: 0.3rem;
  content: '';
  background: url(../images/bg-prev1.png) no-repeat center center;
  background-size: 100% 100%;
}

.swiper-button-prev.swiper-button-disabled:after {
  width: 0.3rem;
  height: 0.3rem;
  content: '';
  background: url(../images/bg-prev.png) no-repeat center center;
  background-size: 100% 100%;
}

.swiper-button-next.swiper-button-disabled:after {
  width: 0.3rem;
  height: 0.3rem;
  content: '';
  background: url(../images/bg-left1.png) no-repeat center center;
  background-size: 100% 100%;
}

.boxtan {
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background-color: rgba(0, 0, 0, .4);
  display: none;
}

.gg {
  width: 600px;
  background: #fff;
  border-radius: 8px;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  z-index: 1000;
  padding: 15px 0 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 0.16rem;
  padding: 0.3rem;
  display: none;
}

.cc {
  width: 6.8rem;
}

h3 {
  text-align: center;
  margin-bottom: 0.1rem;
}
.actile1 h3{
  font-size: 0.4rem;
}
.actile p {
  line-height: 0.25rem;
  width: 100%;
  padding: 10px 30px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 14px;
  color: #666;
  letter-spacing: 0;
  text-align: justify;
  line-height: 21px;
  font-weight: 400;
}

.actile1 p {
  padding: 0;
}

.actile ul li {
  text-indent: 0em;
}

.actile ul li {
  font-size: 14px;
  color: #666;
  padding: 5px 0px;
  text-indent: 1em;
}

.actile .close {
  width: 20px;
  height: 20px;
  background: url(../images/49b17ff.svg) 50%/cover no-repeat;
  position: absolute;
  top: 12px;
  right: 12px;
  cursor: pointer;
}
.swiper-pagination{
  bottom: -0.25rem!important;
}
.swiper-wrapper{
  margin-bottom: 0.3rem;
}

.invalid:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
  color: #c51616;  
}

.invalid::-moz-placeholder { /* Mozilla Firefox 19+ */
  color: #c51616;  
}

input.invalid:-ms-input-placeholder{
  color: #c51616;  
}

input.invalid::-webkit-input-placeholder {
  color: #c51616;  
}
.swiper-pagination-bullet-active {
  background: #fd5545;
}