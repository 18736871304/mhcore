* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.clearfix:after {
  content: "";
  display: table;
  font-size: 0 !important;
  line-height: 0 !important;
  height: 0;
  width: 0;
  clear: both;
}

ul li,
ol li {
  list-style: none;
}

a {
  text-decoration: none;
  outline: 0;
  display: inline-block;
}


input {
  border: 0;
  outline: 0;
}

em,
i {
  font-style: normal;
}

img {
  border: none;
  outline: 0;
}

html {
  font-family: "微软雅黑";
  width: 100%;
}

body {
  width: 100%;
  position: relative;
}




.template {
  width: 100%;
}

.wrap {
  width: 100%;
  background: #ffddda;
}

.wrap img {
  width: 100%;
  height: 100%;
  display: block;
}

.banner img {
  width: 100%;
  height: 100%;
  display: block;
}

.content {
  /* width: 12rem; */
  display: flex;
  justify-content: space-between;
  margin: 0.3rem auto 0;
}

.left-wrap {
  width: 6.8rem;
  margin: 0 auto;
}

.right-wrap {
  width: 3rem;
}

.moudle {
  position: relative;
  margin-bottom: 0.4rem;
  padding-top: 0.18rem;

}
.moudle1 {
  margin-top: 0.4rem;
}


.moudle-header {
  position: absolute;
  top: 3px;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  z-index: 2;
  width: 5.7rem;
  height: 0.975rem;
}

.moudle1 .moudle-header {
  position: absolute;
  top: 5px;
}


.moudle-content {
  background: #fff;
  border-radius: 0.2rem;
  /* padding: 1.7rem 0.5rem 0.8rem; */
  box-sizing: border-box;
}

.moudle-title {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 0.6rem;
}

.moudle-title img {
  width: 0.54rem;
}

.moudle-span {
  line-height: 0.4rem;
  font-size: 0.28rem;
  font-weight: 700;
  color: #333;
  margin: 0 0.2rem;
}

.moudle-text p {
  font-size: 0.22rem;
  color: #333;
  margin-bottom: 0.1rem;
  font-weight: 400;
  text-align: center;
}

.moudle-text p:last-child {
  margin-bottom: 0.6rem;
}


/* 右边的 */




.infor {
  width: 6.8rem;
  height: 9.4rem;
  color: #333;
  font-size: 14px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: url(../images/mobile-bg.png) no-repeat;
  background-size: 100% 100%;
  /* margin-top: 0.18rem; */
  border-radius: 3%;
}

.isFixed {
  position: fixed;
  top: 0;
  margin-top: 0rem;
}

.info-moudle {
  -webkit-box-shadow: 0 5px 8px -2px #c1e2ff;
  box-shadow: 0 5px 8px -2px #c1e2ff;
  width: 6.8rem;
  height: 8.5rem;
  background: #f7fbff;
  margin-top: 1rem;
  border-radius: 0.18rem;
  padding: 0.16rem;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 0.16;
  padding-left: 0.25rem;
}

.label {
  padding-bottom: 0;
  font-size: 14px;
  line-height: 1.5;
  font-weight: 700;
  margin-bottom: 5px;
  float: none;
  display: inline-block;
  text-align: left;
  padding: 0 0 10px;
}

.insureType {
  line-height: 0.4rem;
  position: relative;
  font-size: 14px;
  text-align: left;
}

.insureType ul {
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  max-width: 100%;
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  cursor: pointer;
}

.insureType ul li {
  height: 0.36rem;
  width: 0.6rem;
  border-radius: 0.18rem;
  margin-bottom: 0;
  margin-top: 0;
  box-sizing: border-box;
  font-weight: 400;
  text-align: center;

}

.insureType ul li a,
.insureName .call ul li {
  display: inline-block;
  width: 100%;
  height: 0.7rem;
  line-height: 0.7rem;
  border-radius: 10px;
  border: 1px solid #969696;
  color: #969696;
  background: #fff;
  text-align: center;
  font-size: 14px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-weight: 400;
  cursor: pointer;
}

.insureType ul li .insureSelect,
.insureName .call ul .insureSelect {
  /* background-image: -webkit-gradient(linear, left top, right top, from(#e97c17), to(#fdc635));
  background-image: linear-gradient(90deg, #e97c17, #fdc635); */
  color: #fff;
  border: none;
  background: #ff9956;
}

.insureName {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 0.1rem;
  height: 1rem;
  line-height: 1rem;
}

.insureName .call {
  width: 71%;
}

.insureName .call ul {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.nameValue {
  display: flex;
  position: relative;
  /* flex-direction: column; */
}

.insureName .text {
  height: 0.8rem;
  line-height: 0.8rem;
  font-weight: bold;
  padding-right: 0.1rem;
  font-size: 16px;
}

.insureName .nameValue input {
  width: 4.61rem;
  border: 1px solid #ccc;
  box-sizing: border-box;
  border-radius: 0.08rem;
  font-size: 14px;
  outline: 0;
  padding: 0 0.1rem;
  height: 0.8rem;
  line-height: 0.8rem;
  padding-right: 1rem;
}

.insureName .call ul li:first-child {
  margin-right: 0.1rem;
}

.phoneValue input {
  width: 100%;
  border: 1px solid #ccc;
  box-sizing: border-box;
  border-radius: 0.18rem;
  font-size: 14px;
  outline: 0;
  padding: 0 0.1rem;
  height: 0.36rem;
  line-height: 0.36rem;
  margin-right: 0.15rem;
}

.nameValue em,
.phoneValue em {
  color: #f56c6c;
  font-size: 12px;
  /* display: none; */


}
.nameValue img,
.phoneValue img {
  width: 0.35rem;
  height: 0.35rem;
  position: absolute;
  top: 0.2rem;
  right: 0.2rem;
  display: none;
}
.nameValue .invalid,
.phoneValue .invalid {
  display: block;
}

.nameValue em:before,
.phoneValue em:before {
  content: "";
  width: 14px;
  height: 14px;
  background: url(../images/error.png) no-repeat center center;
  background-size: 100% 100%;
  display: inline-block;
  vertical-align: middle;
  margin-right: 0.05rem;
}

.baofei {
  text-align: center;
  font-size: 0.4rem;
  font-weight: bold;
}

.baofei span {
  font-size: 0.6rem;
  color: #ff7900;
}

.zhu {
  font-size: 0.26rem;
  color: #666666;
  text-align: center;
  margin-top: 0.2rem;
}

.phoneSubmit {
  width: 2.6rem;
  height: 0.55rem;
  line-height: 0.55rem;
  margin-top: 0.2rem;
  /* margin-left: 0.8rem; */
  border: 0;
  background: #FFF;
}

.phoneSubmit1 {
  width: 6.4rem;
  height: 1rem;
  /* margin-left: 0.1rem; */
  margin-bottom: 0.1rem;
}

/* 向此元素应用动画效果 */
.phoneSubmit img {
  width: 90%;
  height: 90%;
  display: block;
  margin: 0 auto;
  animation: myMove 2s infinite;
  -webkit-animation: myMove 2s infinite;
  cursor: pointer;
}

/* .phoneSubmit1 img{
  width: 180%;
  height: 180%;
  display: block;
  margin: 0 auto;
  animation: myMove 2s infinite;
  -webkit-animation: myMove1 2s infinite;
} */
@keyframes myMove {
  0% {
    width: 90%;
    height: 90%;
  }

  50% {
    width: 100%;
    height: 100%;
  }

  100% {
    width: 90%;
    height: 90%;
  }
}

