            <dl class="clearfix my_libiao pr">
                <dt class=" fl fs34">基本保额</dt>
                <dd class="fr">
                    <select  class="" id="amnt" tabindex="-1">
                        <option>请选择</option>
                        <option value="500000">50万</option>
                        <option value="400000">40万</option>
                        <option value="300000">30万</option>
                        <option value="200000">20万</option>
                        <option value="100000">10万</option>
                        <option value="50000">5万</option>
                    </select>
                    <i class="arrow"></i>
                </dd>
                <span class="errorMsg" style="right: 0.25rem;"></span>
            </dl>
            <dl class="clearfix my_libiao my_color" style="border:none">
                <dt class=" fl fs34">产品类型</dt>
                <dd class="fr">
                    <select  class="" id="risktype" tabindex="-1">
                        <option value="zj">重疾</option>
                    </select>
                    <i class="arrow"></i>
                </dd>
                <span class="errorMsg"></span>
