<div class="wrap">
    <div class="bigbox">
        <p class="can_p fs36">被保险人信息</p>
        <div class="libiao1 my_border">
            <dl class="clearfix my_libiao pr" style="">
                <dt class=" fl fs34">年龄</dt>
                <dd class="fr">
                    <input type="number" id="insuredage" maxlength="3" class="my_input" placeholder="请输入年龄">
                </dd>
                <span class="errorMsg"></span>
            </dl>

            <dl class="clearfix my_libiao  pr">
                <dt class=" fl fs34">性别</dt>
                <dd class="fr">
                    <select  class="" id="insuredsex" tabindex="-1">
                    	<option>请选择</option>
                        <option value="1">男</option>
                        <option value="2">女</option>
                    </select>
                    <i class="arrow"></i>
                </dd>
                <span class="errorMsg" style="right: 0.3rem;"></span>
            </dl>
