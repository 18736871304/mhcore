* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.clearfix:after {
  content: "";
  display: table;
  font-size: 0 !important;
  line-height: 0 !important;
  height: 0;
  width: 0;
  clear: both;
}

ul li,
ol li {
  list-style: none;
}

a {
  text-decoration: none;
  outline: 0;
  display: inline-block;
}


input {
  border: 0;
  outline: 0;
}

em,
i {
  font-style: normal;
}

img {
  border: none;
  outline: 0;
}

html {
  font-family: "微软雅黑";
  width: 100%;
}

body {
  width: 100%;
  position: relative;
}




.template {
  width: 100%;
}

.wrap {
  width: 100%;
  background: #e7f3ff;
}

.wrap img {
  width: 100%;
  height: 100%;
  display: block;
}

.banner img {
  width: 100%;
  height: 100%;
  display: block;
}

.content {
  width: 12rem;
  display: flex;
  justify-content: space-between;
  margin: 0.4rem auto 0;
}

.left-wrap {
  width: 8.8rem;
}

.right-wrap {
  width: 3rem;
}

.moudle {
  position: relative;
  margin-bottom: 0.4rem;
  padding-top: 0.18rem;

}

.moudle-header {
  position: absolute;
  top: 3px;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  z-index: 2;
  width: 5.7rem;
  height: 0.975rem;
}

.moudle1 .moudle-header {
  position: absolute;
  top: 5px;
}

.moudle1 .moudle-header {
  background: url(../images/img1-1.png) no-repeat center center;
  background-size: 100% 100%;
}

.moudle2 .moudle-header {
  height: 1.18rem;
  background: url(../images/img2-1.png) no-repeat center center;
  background-size: 100% 100%;
}

.moudle3 .moudle-header {
  height: 1.18rem;
  background: url(../images/img3-1.png) no-repeat center center;
  background-size: 100% 100%;
}

.moudle2 .moudle-content {
  /* padding: 1.24rem 0.54rem 0 0.46rem; */
}

.moudle-content {
  background: #fff;
  border-radius: 0.2rem;
  /* padding: 1.7rem 0.5rem 0.8rem; */
  box-sizing: border-box;
}

.moudle-title {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 0.6rem;
}

.moudle-title img {
  width: 0.54rem;
}

.moudle-span {
  line-height: 0.4rem;
  font-size: 0.28rem;
  font-weight: 700;
  color: #333;
  margin: 0 0.2rem;
}

.moudle-text p {
  font-size: 0.22rem;
  color: #333;
  margin-bottom: 0.1rem;
  font-weight: 400;
  text-align: center;
}

.moudle-text p:last-child {
  margin-bottom: 0.6rem;
}


/* 右边的 */




.infor {
  width: 3.1rem;
  height: 6rem;
  color: #333;
  font-size: 14px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: url(../images/right-bg.png) no-repeat;
  margin-top: 0.4rem;
  border-radius: 3%;
}

.isFixed {
  position: fixed;
  top: 0;
  margin-top: 0rem;
}

.info-moudle {
  -webkit-box-shadow: 0 5px 8px -2px #c1e2ff;
  box-shadow: 0 5px 8px -2px #c1e2ff;
  width: 3.1rem;
  height: 6rem;
  background: #f7fbff;
  margin-top: 0.9rem;
  border-radius: 0.18rem;
  padding: 0.16rem;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 0.16;
  padding-left: 0.25rem;
}

.label {
  padding-bottom: 0;
  font-size: 14px;
  line-height: 1.5;
  font-weight: 700;
  margin-bottom: 5px;
  float: none;
  display: inline-block;
  text-align: left;
  padding: 0 0 10px;
}

.insureType {
  line-height: 0.4rem;
  position: relative;
  font-size: 14px;
  text-align: left;
}

.insureType ul {
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  max-width: 100%;
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  cursor: pointer;
}

.insureType ul li {
  height: 0.36rem;
  width: 0.6rem;
  border-radius: 0.18rem;
  margin-bottom: 0;
  margin-top: 0;
  box-sizing: border-box;
  font-weight: 400;
  text-align: center;

}

.insureType ul li a,
.insureName .call ul li {
  display: inline-block;
  width: 100%;
  height: 0.36rem;
  line-height: 0.36rem;
  border-radius: 10px;
  border: 1px solid #969696;
  color: #969696;
  background: #fff;
  text-align: center;
  font-size: 14px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-weight: 400;
  cursor: pointer;
}

.insureType ul li .insureSelect,
.insureName .call ul .insureSelect {
  /* background-image: -webkit-gradient(linear, left top, right top, from(#e97c17), to(#fdc635));
  background-image: linear-gradient(90deg, #e97c17, #fdc635); */
  color: #fff;
  border: none;
  background: #0692f7;
}

.insureName {
  display: flex;
  margin-bottom: 0.2rem;
}

.insureName .call {
  width: 64%;
}

.insureName .call ul {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.nameValue {
  display: flex;
  /* flex-direction: column; */
  position: relative;
}

.nameValue img {
  width: 0.2rem;
  height: 0.2rem;
  position: absolute;
  right: 0.1rem;
  top: 0.1rem;
  display: none;
}

.nameValue .invalid,
.phoneValue .invalid {
  display: block;
}

.insureName .text {
  height: 0.36rem;
  line-height: 0.36rem;
  font-weight: bold;
  padding-right: 0.1rem;
  font-size: 16px;
}

.insureName .nameValue input {
  width: 1.77rem;
  border: 1px solid #ccc;
  box-sizing: border-box;
  border-radius: 0.08rem;
  font-size: 14px;
  outline: 0;
  padding: 0 0.1rem;
  height: 0.42rem;
  line-height: 0.42rem;
  padding-right: 0.4rem;
}

.insureName .call ul li:first-child {
  margin-right: 0.1rem;
}

.phoneValue input {
  width: 100%;
  border: 1px solid #ccc;
  box-sizing: border-box;
  border-radius: 0.18rem;
  font-size: 14px;
  outline: 0;
  padding: 0 0.1rem;
  height: 0.36rem;
  line-height: 0.36rem;
  margin-right: 0.15rem;
}

.nameValue em,
.phoneValue em {
  color: #f56c6c;
  font-size: 12px;
  display: none;
}

.nameValue em:before,
.phoneValue em:before {
  content: "";
  width: 14px;
  height: 14px;
  background: url(../images/error.svg) no-repeat center center;
  background-size: 100% 100%;
  display: inline-block;
  vertical-align: middle;
  margin-right: 0.05rem;
}

.baofei {
  text-align: center;
  font-size: 0.2rem;
  font-weight: bold;
}

.baofei span {
  font-size: 0.26rem;
  color: #ff7900;
}

.zhu {
  font-size: 0.12rem;
  color: #666666;
  text-align: center;
  margin-top: 0.15rem;
}


.phoneSubmit {
  width: 2.6rem;
  height: 0.55rem;
  line-height: 0.55rem;
  margin-top: 0.2rem;
  margin-bottom: 0.04rem;
  border: 0;
  background: #FFF;
}


/* 向此元素应用动画效果 */
.phoneSubmit img {
  width: 80%;
  height: 80%;
  display: block;
  margin: 0 auto;
  animation: myMove 3s infinite;
  -webkit-animation: myMove 3s infinite;
  cursor: pointer;
}

@keyframes myMove {
  0% {
    width: 80%;
    height: 80%;
  }

  50% {
    width: 100%;
    height: 100%;
  }

  100% {
    width: 80%;
    height: 80%;
  }
}

.xieyi {
  margin-top: 0.1rem;
  font-size: 0.13rem;
  color: #999;
  text-align: justify;
}

.bigBtn {
  cursor: pointer;
  position: relative;
  padding-bottom: 20px;
}

.bigBtn img {
  width: 5.45rem;
  height: 0.95rem;
  display: block;
  margin: 0 auto;
}

.moudle3 .moudle-content {
  padding-bottom: 0.3rem;
}

.lunbo {
  width: 80%;
  padding: 0.4rem;
  margin: 0 auto;
  border: 2px solid #eaeaea;
  position: relative;
  border-radius: 0.3rem;
}



.swiper-slide {
  text-align: center;
  font-size: 18px;
  background: #fff;

  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.swiper-slide img {
  display: block;
  width: 100%;
  height: 100%;
  object-fit: cover;
}



.swiper-button-next {
  right: 5px;
  left: auto;
}

.swiper-button-prev {
  left: 5px;
  right: auto;
}

.swiper-button-next:after {
  width: 0.3rem;
  height: 0.28rem;
  content: '';
  background: url(../images/bg-left.png) no-repeat center center;
  background-size: 100% 100%;
}

.swiper-button-prev:after {
  width: 0.3rem;
  height: 0.28rem;
  content: '';
  background: url(../images/bg-prev1.png) no-repeat center center;
  background-size: 100% 100%;
}

.swiper-button-prev.swiper-button-disabled:after {
  width: 0.3rem;
  height: 0.28rem;
  content: '';
  background: url(../images/bg-prev.png) no-repeat center center;
  background-size: 100% 100%;
}

.swiper-button-next.swiper-button-disabled:after {
  width: 0.3rem;
  height: 0.28rem;
  content: '';
  background: url(../images/bg-left1.png) no-repeat center center;
  background-size: 100% 100%;
}

.boxtan,
.success {
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background-color: rgba(0, 0, 0, .4);
  display: none;
}

.gg,
.dd {
  width: 900px;
  background: #fff;
  border-radius: 8px;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  z-index: 1000;
  padding: 15px 0 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 0.16rem;
  padding: 0.3rem;
  display: none;
}

.dd {
  padding: 0;
}

.err {
  width: 0.3rem;
  height: 0.3rem;
  position: absolute;
  top: -0.1rem;
  right: -0.1rem;
  cursor: pointer;
}

.err img {
  width: 0.3rem;
  height: 0.3rem;
  position: absolute;
  top: -0.15rem;
  right: -0.1rem;
  cursor: pointer;
}

h3 {
  text-align: center;
  margin-bottom: 0.1rem;
}

.actile p {
  line-height: 0.25rem;
  width: 100%;
  padding: 10px 30px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 14px;
  color: #666;
  letter-spacing: 0;
  text-align: justify;
  line-height: 21px;
  font-weight: 400;
}

.actile ul li {
  font-size: 14px;
  color: #666;
  padding: 2px 30px;
  text-indent: 1em;
}

.actile .close {
  width: 20px;
  height: 20px;
  background: url(../images/49b17ff.svg) 50%/cover no-repeat;
  position: absolute;
  top: 12px;
  right: 12px;
  cursor: pointer;
}

.invalid:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: #c51616;
}

.invalid::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: #c51616;
}

input.invalid:-ms-input-placeholder {
  color: #c51616;
}

input.invalid::-webkit-input-placeholder {
  color: #c51616;
}