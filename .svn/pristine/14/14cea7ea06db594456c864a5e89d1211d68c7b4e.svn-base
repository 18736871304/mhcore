* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.clearfix:after {
  content: "";
  display: table;
  font-size: 0 !important;
  line-height: 0 !important;
  height: 0;
  width: 0;
  clear: both;
}

ul li,
ol li {
  list-style: none;
}

a {
  text-decoration: none;
  outline: 0;
  display: inline-block;
}


input {
  border: 0;
  outline: 0;
}

em,
i {
  font-style: normal;
}

img {
  border: none;
  outline: 0;
}

html {
  font-family: "微软雅黑";
  width: 100%;
}

body {
  width: 100%;
  position: relative;
}




.template {
  width: 100%;
}

.wrap {
  width: 100%;
  background: #0078ff;
  /* margin-bottom: 1.1rem; */
}

.wrap img {
  width: 100%;
  height: 100%;
  display: block;
}

.banner img,
.bottom img {
  width: 100%;
  height: 100%;
  display: block;
}

.content {
  /* width: 12rem; */
  display: flex;
  justify-content: space-between;
  margin: 0.45rem auto 0;
}

.left-wrap {
  width: 6.8rem;
  margin: 0 auto;
}


/* 右边的 */
.infor {
  color: #333;
  font-size: 14px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  margin: 0 auto;
  border-top-left-radius: 4%;
  border-top-right-radius: 4%;
  background: #fff;
}

.info-moudle {
  width: 100%;
  height: 100%;
  padding: 0.4rem 0.6rem;
}

.isFixed {
  position: fixed;
  top: 0;
  margin-top: 0rem;
}

.insureName {
  display: flex;
  /* justify-content: center; */
  align-items: center;
  margin-bottom: 0.2rem;
  height: 1rem;
  line-height: 1rem;
}

.insureName .call {
  width: 75%;
}

.insureName .call ul {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.nameValue {
  display: flex;
  flex-direction: column;
  position: relative;
}

.insureName .text {
  height: 0.7rem;
  line-height: 0.7rem;
  font-weight: bold;
  padding-right: 0.3rem;
}

.insureName .nameValue input {
  width: 4.8rem;
  border: 1px solid #ccc;
  box-sizing: border-box;
  border-radius: 0.08rem;
  font-size: 16px;
  outline: 0;
  padding: 0 0.1rem;
  height: 0.9rem;
  line-height: 0.9rem;
  /* margin-right: 0.15rem; */
}

.insureName .call ul li:last-child {
  margin-right: 0rem;
}

.insureName .call ul li {
  width: 1.53rem;
  height: 0.72rem;
  line-height: 0.72rem;
  text-align: center;
  border: 1px solid #d8d8d8;
  border-radius: 8%;
  margin-right: 0.1rem;
  font-weight: bold;
}

.insureSelect {
  background: #fe5d17;
  color: #fff;
}




.phoneSubmit {
  width: 6.1rem;
  height: 0.9rem;
  line-height: 0.9rem;
  margin-top: 0.2rem;
  border: 0;
  background: #FFF;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.phoneSubmit .title {
  width: 6.1rem;
  height: 0.9rem;
  background: #fe5d17;
  color: #fff;
  text-align: center;
  letter-spacing: 2px;
  font-weight: bold;
  font-size: 18px;
}

.nameValue img {
  position: absolute;
  width: 0.35rem;
  height: 0.35rem;
  position: absolute;
  top: 0.2rem;
  right: 0.2rem;
  display: none;
}

.nameValue img {
  width: 0.4rem;
  height: 0.4rem;
  position: absolute;
  top: 0.25rem;
  right: 0.2rem;
  display: none;
}

.info-moudle .invalid{
  display: block;
}









.success {
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background-color: rgba(0, 0, 0, .4);
  display: none;
}


.dd {
  width: 5.4rem;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  z-index: 1000;
  padding: 15px 0 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: none;
}

